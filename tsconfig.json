{
  "extends": ["@vue/tsconfig/tsconfig.lib.json", "@vue/tsconfig/tsconfig.dom.json"],
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "sourceMap": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "types": ["vite/client"],
    "baseUrl": "./"
  },
  "include": [
    ".vitepress/**/*.ts",
    ".vitepress/**/*.d.ts",
    ".vitepress/**/*.tsx",
    ".vitepress/**/*.vue"
  ],
  "exclude": ["**/dist/**", "**/node_modules/**"]
}
